{
  "name": "temporal",
  "productName": "Temporal",
  "private": true,
  "version": "1.0.0-alpha.1",
  "description": "Budgeting app with a sense of time.",
  "author": "jacksonsalopek",
  "license": "MIT",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "build": "npm run typecheck && node scripts/build.mjs && electron-builder --config .electron-builder.config.js",
    "coverage:unit": "jest --config config/test/unit.ts --coverage",
    "format": "rome format --write packages/**/*.ts",
    "lint": "rome check packages/**/*.ts",
    "lint:fix": "rome check --apply packages/**/*.ts",
    "test": "pnpm test:unit",
    "test:components": "jest --config config/test/components.ts",
    "test:unit": "jest --config config/test/unit.ts",
    "typecheck": "tsc --noEmit --project packages/renderer/tsconfig.json"
  },
  "engines": {
    "node": ">=18.15.0",
    "pnpm": ">=8.6.0"
  },
  "dependencies": {
    "@18f/us-federal-holidays": "^4.0.0",
    "@fullcalendar/core": "^6.1.8",
    "@fullcalendar/daygrid": "^6.1.8",
    "@fullcalendar/interaction": "^6.1.8",
    "@fullcalendar/rrule": "^6.1.8",
    "@fullcalendar/web-component": "^6.1.8",
    "@solidjs/router": "^0.8.2",
    "daisyui": "^2.51.5",
    "electron-store": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rrule": "^2.7.2",
    "solid-icons": "^1.0.11",
    "solid-styled-components": "^0.28.5"
  },
  "packageManager": "pnpm@8.0.0",
  "devDependencies": {
    "@solidjs/testing-library": "^0.8.4",
    "@swc/jest": "^0.2.28",
    "@tailwindcss/typography": "^0.5.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.18",
    "@types/react-dom": "^18.2.7",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.6.2",
    "electron": "^25.4.0",
    "electron-builder": "^24.6.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "postcss": "^8.4.27",
    "rome": "^12.1.3",
    "solid-app-router": "^0.4.2",
    "solid-devtools": "^0.27.7",
    "solid-element": "^1.7.1",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.7.9",
    "solid-styled-jsx": "^0.27.1",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-resolve": "^2.3.0",
    "vite-plugin-solid": "^2.7.0"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344,
    "APP_NAME": "Temporal"
  }
}
